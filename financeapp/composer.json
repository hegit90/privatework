{
    "name": "financeapp/financial-management-system",
    "description": "Production-grade personal financial and gig income management system",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "vlucas/phpdotenv": "^5.5",
        "phpmailer/phpmailer": "^6.8",
        "firebase/php-jwt": "^6.8",
        "mpdf/mpdf": "^8.2",
        "phpoffice/phpspreadsheet": "^1.29",
        "bacon/bacon-qr-code": "^2.0",
        "spatie/regex": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
